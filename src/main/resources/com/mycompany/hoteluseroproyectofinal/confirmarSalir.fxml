<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.hoteluseroproyectofinal.ConfirmarSalirController">
   <children>
      <Label layoutX="26.0" layoutY="79.0" prefHeight="89.0" prefWidth="585.0" text="Esta seguro que quiere salir?" textFill="#da0e0e">
         <font>
            <Font name="System Bold Italic" size="41.0" />
         </font>
      </Label>
      <Button fx:id="btnSi" layoutX="150.0" layoutY="223.0" mnemonicParsing="false" onAction="#accionSi" text="Si">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
      <Button fx:id="btnNo" layoutX="348.0" layoutY="223.0" mnemonicParsing="false" onAction="#accionNo" prefHeight="65.0" prefWidth="89.0" text="No">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
